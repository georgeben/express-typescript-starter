{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "Quickly create an Express server using Typescript without wasting time on boilerplate code",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register src/test/**/*.spec.ts --timeout=10000 --exit",
    "start:dev": "node dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm run build:dev\" \"npm run start:dev\""
  },
  "keywords": ["express", "typescript"],
  "author": "Kurobara George Benjamin",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.3.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lowdb": "^1.0.0",
    "morgan": "^1.9.1",
    "nanoid": "^2.1.11"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/supertest": "^2.0.8",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.3",
    "@types/lowdb": "^1.0.9",
    "@types/morgan": "^1.9.0",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.9.2",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
    "mocha": "^7.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  }
}
